Bookmarks
<ul>
<% Bookmark.all.each do |hash| %>
<li><a href='<%=hash[:url]%>'><%=hash[:title]%></a></li>
<%end%>
</ul>
<form action='/add' method='post'>
  <input type='text' name='url' placeholder="Enter a url" required>
  <input type='text' name="title" placeholder="Enter the url title" required>
  <input type='submit' value='Add'>
</form>

<form action='/edit' method='get'>
  <input type='submit' value='Edit Bookmarks'>
</form>